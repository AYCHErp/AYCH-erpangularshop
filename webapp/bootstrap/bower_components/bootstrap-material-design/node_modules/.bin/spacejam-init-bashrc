#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//bin/bash" ]; then
  "$basedir//bin/bash"  -xe "$basedir/../spacejam/bin/spacejam-init-bashrc" "$@"
  ret=$?
else 
  /bin/bash  -xe "$basedir/../spacejam/bin/spacejam-init-bashrc" "$@"
  ret=$?
fi
exit $ret
